	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Current File (Integrated Terminal)",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal",
			"args": [
				"-v",
				"-p", "ttyACM0",
				"-b", "921600",
				"-r",
				"--reconnect-delay", "1",
				// "--trace-error",
			],
		},
	]
}
